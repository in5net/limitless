{
  "name": "@limitlesspc/limitless",
  "version": "1.1.6",
  "author": {
    "name": "Izaak Brown",
    "email": "izaakbthunderbird@gmail.com"
  },
  "repository": {
    "url": "git@github.com:limitlesspc/limitless.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "module": "./index.mjs",
  "main": "./index.js",
  "types": "./index.d.ts",
  "scripts": {
    "preinstall": "npx only-allow npm",
    "setup": "chmod +x ./mjs.sh",
    "dev": "parcel examples/**/index.html",
    "build": "tsc && tsc -p ./tsconfig.esm.json && ./mjs.sh",
    "lint": "eslint src/ --ext .ts",
    "test": "mocha -r ts-node/register src/**/*.test.ts",
    "pub": "npm run build && cp package.json build/ && cd build/ && npm publish"
  },
  "dependencies": {
    "p5": "^1.3.1"
  },
  "devDependencies": {
    "@types/chai": "^4.2.17",
    "@types/mocha": "^8.2.2",
    "@types/p5": "^0.9.1",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "chai": "^4.3.4",
    "eslint": "^7.25.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "glslify-bundle": "^5.1.1",
    "glslify-deps": "^1.3.2",
    "mocha": "^8.3.2",
    "parcel-bundler": "^1.12.5",
    "ts-node": "^9.1.1",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  }
