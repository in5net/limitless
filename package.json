{
  "name": "@limitlesspc/limitless",
  "version": "1.2.8",
  "author": {
    "name": "Izaak Brown",
    "email": "izaakbthunderbird@gmail.com"
  },
  "repository": "https://github.com:limitlesspc/limitless.git",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "files": [
    "dist"
  ],
  "module": "./dist/limitless.es.js",
  "main": "./dist/limitless.umd.js",
  "types": "./dist/limitless.d.ts",
  "exports": {
    ".": {
      "import": "./dist/limitless.es.js",
      "require": "./dist/limitless.umd.js"
    }
  },
  "scripts": {
    "setup": "chmod +x ./mjs.sh",
    "dev": "parcel examples/**/index.html",
    "build": "vite build && tsup src/limitless.ts --dts && rm dist/limitless.js",
    "lint": "eslint src/ --ext .ts",
    "test": "ts-mocha src/**/*.test.ts",
    "pub": "npm run build && npm publish"
  },
  "browserslist": [
    "last 1 Chrome version"
  ],
  "dependencies": {
    "gl-matrix": "^3.3.0",
    "p5": "^1.4.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.21",
    "@types/mocha": "^9.0.0",
    "@types/p5": "^1.3.0",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "chai": "^4.3.4",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.0",
    "glslify-bundle": "^5.1.1",
    "glslify-deps": "^1.3.2",
    "mocha": "^9.0.3",
    "parcel-bundler": "^1.12.5",
    "ts-mocha": "^8.0.0",
    "tslib": "^2.3.1",
    "tsup": "^4.13.1",
    "typescript": "^4.3.5",
    "vite": "^2.4.4"
  }
}
