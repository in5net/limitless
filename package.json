    "registry": "https://npm.pkg.github.com/"
  },
  "module": "./index.mjs",
  "main": "./index.js",
  "types": "./index.d.ts",
  "scripts": {
    "setup": "chmod +x ./mjs.sh",
    "dev": "parcel examples/**/index.html",
