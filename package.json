{
  "name": "@limitlesspc/limitless",
  "version": "1.3.14",
  "type": "module",
  "author": {
    "name": "Izaak Brown",
    "email": "izaakbthunderbird@gmail.com"
  },
  "repository": "github:limitlesspc/limitless",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "files": [
    "dist"
  ],
  "module": "./dist/limitless.es.js",
  "main": "./dist/limitless.umd.js",
  "types": "./dist/limitless.d.ts",
  "exports": {
    ".": {
      "import": "./dist/limitless.es.js",
      "require": "./dist/limitless.umd.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-s build:*",
    "build:vite": "vite build",
    "build:types": "tsup src/limitless.ts --dts && rm dist/limitless.cjs",
    "lint": "eslint --ignore-path .gitignore .",
    "test": "mocha -r ts-node/register src/**/*.test.ts",
    "pub": "run-s lint build && npm publish"
  },
  "dependencies": {
    "gl-matrix": "^3.4.3",
    "p5": "^1.4.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.22",
    "@types/mocha": "^9.0.0",
    "@types/p5": "^1.3.1",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "chai": "^4.3.4",
    "eslint": "^8.3.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "mocha": "^9.1.3",
    "npm-run-all": "^4.1.5",
    "ts-node": "^10.4.0",
    "tslib": "^2.3.1",
    "tsup": "^5.8.0",
    "typescript": "^4.5.2",
    "vite": "^2.6.14"
  }
}
