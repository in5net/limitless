{
  "name": "@limitlesspc/limitless",
  "version": "1.3.0",
  "author": {
    "name": "Izaak Brown",
    "email": "izaakbthunderbird@gmail.com"
  },
  "repository": "https://github.com:limitlesspc/limitless.git",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "files": [
    "dist"
  ],
  "module": "./dist/limitless.es.js",
  "main": "./dist/limitless.umd.js",
  "types": "./dist/limitless.d.ts",
  "exports": {
    ".": {
      "import": "./dist/limitless.es.js",
      "require": "./dist/limitless.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "run-s build:*",
    "build:vite": "vite build",
    "build:types": "tsup src/limitless.ts --dts && rm dist/limitless.js",
    "lint": "run-s lint:*",
    "lint:eslint": "eslint --ignore-path .gitignore .",
    "lint:svelte": "svelte-check --tsconfig ./tsconfig.json",
    "test": "ts-mocha src/**/*.test.ts",
    "pub": "run-s build && npm publish"
  },
  "dependencies": {
    "gl-matrix": "^3.3.0",
    "p5": "^1.4.0"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.17",
    "@types/chai": "^4.2.21",
    "@types/mocha": "^9.0.0",
    "@types/p5": "^1.3.0",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "chai": "^4.3.4",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-svelte3": "^3.2.0",
    "mocha": "^9.0.3",
    "npm-run-all": "^4.1.5",
    "sass": "^1.37.5",
    "svelte": "^3.42.1",
    "svelte-check": "^2.2.4",
    "svelte-preprocess": "^4.7.4",
    "ts-mocha": "^8.0.0",
    "tslib": "^2.3.1",
    "tsup": "^4.13.1",
    "typescript": "^4.3.5",
    "vite": "^2.4.4"
  }
}
