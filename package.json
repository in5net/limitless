{
  "name": "@limitlesspc/limitless",
  "version": "1.3.21",
  "type": "module",
  "author": {
    "name": "Izaak Brown",
    "email": "izaakbthunderbird@gmail.com"
  },
  "repository": "github:limitlesspc/limitless",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "files": [
    "dist"
  ],
  "module": "./dist/index.js",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    "./*": {
      "import": "./dist/*.js",
      "require": "./dist/*.js",
      "types": "./dist/*.d.ts"
    },
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "run-s build:*",
    "build:clean": "rm -rf dist",
    "build:ts": "tsc",
    "lint": "eslint --ignore-path .gitignore .",
    "test": "ava",
    "pub": "run-s lint build && npm publish"
  },
  "ava": {
    "files": [
      "src/**/*.test.ts"
    ],
    "timeout": "1m",
    "nonSemVerExperiments": {
      "nextGenConfig": true,
      "configurableModuleFormat": true
    },
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=ts-node/esm"
    ]
  },
  "dependencies": {
    "axios": "^0.24.0",
    "cheerio": "^1.0.0-rc.10",
    "gl-matrix": "^3.4.3",
    "p5": "^1.4.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/p5": "^1.3.2",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "ava": "^3.15.0",
    "chai": "^4.3.4",
    "eslint": "^8.5.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "npm-run-all": "^4.1.5",
    "ts-node": "^10.4.0",
    "tslib": "^2.3.1",
    "tsup": "^5.11.6",
    "typescript": "^4.5.4",
    "vite": "^2.7.3"
  }
}
